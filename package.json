{
  "name": "mepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:clean": "cd android/ && ./gradlew clean",
    "android:run": "ENVFILE=.env.development react-native run-android",
    "android:run:staging": "ENVFILE=.env.staging react-native run-android",
    "android:run:prod": "ENVFILE=.env.production react-native run-android",
    "android:run:release": "ENVFILE=.env.production react-native run-android --variant=release",
    "android:build:release": "cd android/ && ENVFILE=.env.production ./gradlew app:assembleRelease",
    "android:build:dev": "cd android/ && ENVFILE=.env.development ./gradlew app:assembleRelease",
    "android:build:staging": "cd android/ && ENVFILE=.env.staging ./gradlew app:assembleRelease",
    "android:build:debug": "ENVFILE=.env.development react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/; cd android && ./gradlew assembleDebug && cd ..",
    "android:bundle:release": "cd android/ && ENVFILE=.env.production ./gradlew app:bundleRelease && cd ..",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-seoul/masonry-list": "^1.4.2",
    "@react-navigation/bottom-tabs": "^6.0.3",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^1.3.4",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-blurhash": "^1.1.10",
    "react-native-config": "^1.5.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-modal": "^2.0.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "^3.2.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-redux": "^7.1.25",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.162.0",
    "react-test-renderer": "18.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-flipper": "^2.0.2",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
